{
	"__COMMENT__ More Information": "https://github.com/mozilla/policy-templates/blob/master/README.md",
	"policies": {
		"AppUpdateURL": "https://localhost",
		"Cookies": {
			"Allow": [
				"https://example.com",
				"https://localhost"
			],
			"Behavior": "reject-tracker-and-partition-foreign",
			"Locked": false
		},
		"SanitizeOnShutdown": {
			"Cache": true,
			"Cookies": false,
			"Downloads": false,
			"FormData": true,
			"History": false,
			"OfflineApps": true,
			"Sessions": true,
			"SiteSettings": false,
			"Locked": false
		},
		"EncryptedMediaExtensions": {
			"Enabled": true,
			"Locked": false
		},
		"DisableAppUpdate": true,
		"OverrideFirstRunPage": "",
		"OverridePostUpdatePage": "",
		"DisableSystemAddonUpdate": true,
		"DisableProfileImport": false,
		"DisableFirefoxStudies": true,
		"DisableTelemetry": true,
		"DisableFeedbackCommands": true,
		"DisablePocket": true,
		"DisableSetDesktopBackground": true,
		"DisableDeveloperTools": false,
		"NoDefaultBookmarks": true,
		"Extensions": {
			"Install": [
				"https://addons.mozilla.org/firefox/downloads/latest/ublock-origin/latest.xpi",
				"https://github.com/Heptazhou/firefox-qr/releases/latest/download/latest.xpi"
			],
			"Uninstall": []
		},
		"SearchEngines": {
			"PreventInstalls": false,
			"Remove": ["Google"],
			"Default": "Duckduckgo.com",
			"Add": [
				{
					"Name": "Duckduckgo.com",
					"Description": "Search Duckduckgo.com",
					"Alias": "@ddg",
					"Method": "GET",
					"URLTemplate": "https://duckduckgo.com/?k1=-1&kaj=m&kak=-1&kao=-1&kaq=-1&kau=-1&kn=1&kp=-2&q={searchTerms}",
					"SuggestURLTemplate": "https://ac.duckduckgo.com/ac/?q={searchTerms}",
					"IconURL": "data:image/png;base64,"
				},
				{
					"Name": "Google.com",
					"Description": "Search Google.com",
					"Alias": "@goog",
					"Method": "GET",
					"URLTemplate": "https://www.google.com/search?hl={language}&filter=0&newwindow=1&q={searchTerms}",
					"SuggestURLTemplate": "https://www.google.com/complete/search?q={searchTerms}",
					"IconURL": "data:image/png;base64,"
				}
			]
		},
		"SupportMenu": {
			"Title": "Snowfox Issue Tracker",
			"URL": "https://github.com/Heptazhou/Snowfox/issues"
		}
	}
}
